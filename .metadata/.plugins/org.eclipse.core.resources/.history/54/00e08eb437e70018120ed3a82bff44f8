package Exceptions;

public class NestedTryBlocks {
	
	public static void main(String[] args) {
		
		
		
		
	}
	
	
	public static void nestedTryBlock() {
		try {
			  try {
			    throw new Error("oops");
			  }
			  finally {
			   
			    System.out.println("finally");
			  }
			}catch (Exception ex) {
				 
				System.out.println(ex.getMessage());
			  
			}
	}

}
